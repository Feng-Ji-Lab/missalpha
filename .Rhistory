summary_display_all <- function(results) {
for (name in names(results)) {
result <- results[[name]]
cat(sprintf("%s:\n", name))
cat(sprintf("Alpha Min: %f\n", result$Alpha_Min))
cat(sprintf("Alpha Max: %f\n\n", result$Alpha_Max))
}
}
